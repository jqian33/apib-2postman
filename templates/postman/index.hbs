{
    "info": {
        "name": {{{json title}}},
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "auth": {
        "type": "bearer",
        "bearer": [
            {
                "key": "token",
                "value": "\{{access_token}}",
                "type": "string"
            }
        ]
    },
    "item": [
        {{#items}}
        {{> item~}}
        {{#unless @last}},{{/unless}}
        {{/items}}
    ]
}